    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-plugin-html": "^4.0.3",
