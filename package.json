    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4",
    "wepy-compiler-babel": "^1.5.1",
    "wepy-compiler-pug": "^1.4.7",
    "wepy-compiler-stylus": "^1.3.10",
