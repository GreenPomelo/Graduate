module.exports = {
  filter: function(name) {
    if (name.indexOf("doc") > -1) {
      return "https://static.airbob.org/admin/news_pic_word.png";
    }
    if (name.indexOf("xls") > -1) {
      return "https://static.airbob.org/admin/news_pic_excel.png";
    }
    if (name.indexOf("pdf") > -1) {
      return "https://static.airbob.org/admin/news_pic_pdf.png";
    }
  }
};
